// Potentially helpful for future VSCode settings.json file
{
    "workbench.colorTheme": "Gruvbox Dark Soft",
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.cwd": "C:\\developer\\repos",
    "editor.inlineSuggest.enabled": true,
    "terminal.explorerKind": "external",
    "vim.vimrc.enable": true,
    "vim.leader": " ",
    "vim.vimrc.path": "~/AppData/Local/nvim/.vscodevimrc",
    "vim.highlightedyank.enable": true,
    "vim.smartRelativeLine": true,
    "vim.incsearch": true,
    "vim.showcmd": true,
    "editor.tabSize": 2,
    "vim.hlsearch": false,
    "vim.normalModeKeyBindingsNonRecursive":[
      { "before": ["<leader>", "r", "c"], "commands": ["workbench.action ReformatCode"] },
      { "before": ["<leader>", "r", "w"], "commands": ["workbench.action RenameElement"] },
      { "before": ["<C-k>"], "commands": ["workbench.action GotoPreviousError"] },
      { "before": ["<C-j>"], "commands": ["workbench.action GotoNextError"] },
      { "before": ["<C-e>"], "commands": ["workbench.action ShowErrorDescription"] },
      { "before": ["<leader>", "h"], "commands": ["workbench.action HideAllWindows"] },
      { "before": ["<leader>", "H"], "commands": ["workbench.action ToggleDistractionFreeMode"] },
      { "before": ["<leader>", "l"], "commands": ["workbench.action ChangeToolWindowLayout"] },
      { "before": ["<leader>", "L"], "commands": ["workbench.action StoreNewLayout"] },
      { "before": ["<leader>", "e"], "commands": ["workbench.action CloseAllEditorsButActive"] },
      { "before": ["<leader>", "-"], "commands": ["workbench.action ToggleZenMode"] },
      { "before": ["<leader>", "_"], "commands": ["workbench.action TogglePresentationMode"] },
      { "before": ["<leader>", "f", "s"], "commands": ["workbench.action GotoFile"] },
      { "before": ["<leader>", "f", "o"], "commands": ["workbench.action OpenFile"] },
      { "before": ["<leader>", "f", "r"], "commands": ["workbench.action RenameFile"] },
      { "before": ["<leader>", "f", "n"], "commands": ["workbench.action NewFile"] },
      { "before": ["<leader>", "f", "p"], "commands": ["workbench.action SelectInProjectView"] },
      { "before": ["<leader>", "?"], "commands": ["workbench.action RecentFiles"] },
      { "before": ["<leader>", "<space>"], "commands": ["workbench.action RecentFiles"] },
      { "before": ["<C-w>", "<C-q>"], "commands": ["workbench.action CloseEditor"] },
      { "before": ["<C-w>", "q"], "commands": ["workbench.action CloseEditor"] },
      { "before": ["<C-w>", "H"], "commands": ["workbench.action MoveEditorToOppositeTabGroup"] },
      { "before": ["<C-w>", "J"], "commands": ["workbench.action MoveEditorToOppositeTabGroup"] },
      { "before": ["<C-w>", "K"], "commands": ["workbench.action MoveEditorToOppositeTabGroup"] },
      { "before": ["<C-w>", "L"], "commands": ["workbench.action MoveEditorToOppositeTabGroup"] },
      { "before": ["<C-w>", "V"], "commands": ["workbench.action MoveTabRight"] },
      { "before": ["<C-w>", "S"], "commands": ["workbench.action MoveTabDown"] },
      { "before": ["<C-=>"], "commands": ["workbench.action EditorIncreaseFontSize"] },
      { "before": ["<C-->"], "commands": ["workbench.action EditorDecreaseFontSize"] },
      { "before": ["z", "c"], "commands": ["workbench.action CollapseRegion"] },
      { "before": ["z", "C"], "commands": ["workbench.action CollapseAllRegions"] },
      { "before": ["z", "o"], "commands": ["workbench.action ExpandRegion"] },
      { "before": ["z", "O"], "commands": ["workbench.action ExpandAllRegions"] },
      { "before": ["z", "a"], "commands": ["workbench.action CollapseSelection"] },
      { "before": ["<leader>", "s", "g"], "commands": ["workbench.action SearchEverywhere"] },
      { "before": ["<leader>", "s", "u"], "commands": ["workbench.action FindUsages"] },
      { "before": ["<leader>", "s", "w"], "commands": ["workbench.action CheckinProject"] },
      { "before": ["<C-/>"], "commands": ["workbench.action CommentByLineComment"] },
      { "before": ["<leader>", "<C-/>"], "commands": ["workbench.action CommentByBlockComment"] },
      { "before": ["<leader>", "b", "b"], "commands": ["workbench.action MakeGradleProject"] },
      { "before": ["<leader>", "b", "r"], "commands": ["workbench.action RebuildGradleProject"] },
      { "before": ["<leader>", "b", "c"], "commands": ["workbench.action CleanGradleProject"] },
      { "before": ["<leader>", "r", "r"], "commands": ["workbench.action Run"] },
      { "before": ["<leader>", "r", "d"], "commands": ["workbench.action Debug"] },
      { "before": ["<leader>", "r", "c"], "commands": ["workbench.action Resume"] },
      { "before": ["<leader>", "r", "s"], "commands": ["workbench.action Stop"] },
      { "before": ["<leader>", "r", "a", "d"], "commands": ["workbench.action XDebugger.AttachToProcess"] },
      { "before": ["\\", "b"], "commands": ["workbench.action ToggleLineBreakpoint"] },
      { "before": ["g;"], "commands": ["workbench.action JumpToLastChange"] },
      { "before": ["g,"], "commands": ["workbench.action JumpToNextChange"] },
    ],
    "vim.visualModeKeyBindingsNonRecursive":[
      { "before": ["J"], "commands": ["workbench.action MoveStatementDown"] },
      { "before": ["K"], "commands": ["workbench.action MoveStatementUp"] },
      { "before": ["<leader>", "r", "c"], "commands": ["workbench.action ReformatCode"] },
      { "before": ["<C-/>"], "commands": ["workbench.action CommentByLineComment"] },
      { "before": ["<leader>", "<C-/>"], "commands": ["workbench.action CommentByBlockComment"] },
      { "before": ["<leader>", "I"], "commands": ["workbench.action EditorAddCaretPerSelectedLine"] },
      { "before": ["<leader>", "A"], "commands": ["workbench.action EditorAddCaretPerSelectedLine"] },
    ],
    "vim.insertModeKeyBindingsNonRecursive":[],
}
